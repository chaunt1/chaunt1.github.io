{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "plugin:security/recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "security", "prettier"],
  "rules": {
    //here we can change any rule "off/error/warn"
    "prettier/prettier": "error", // or warn
    "no-unused-vars": "warn" // default in airbnb is error
  }
}
