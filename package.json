{
  "name": "chaunt1.github.io",
  "homepage": "chaunt1.github.io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "dev-staging": "bunx --bun vite --mode staging",
    "build": "tsc && bunx --bun vite build",
    "build-dev": "tsc && bunx --bun vite build --mode development",
    "build-test": "tsc && bunx --bun vite build --mode test",
    "build-staging": "tsc && bunx --bun vite build --mode staging",
    "preview": "bunx --bun vite preview",
    "prepare": "husky install",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx} --fix",
    "prettier": "prettier --check './src/**/*.{js,jsx, json, css, scss, md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx, json, css, scss, md}'"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fluentui/react-components": "^9.35.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.8.5",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "js-cookie": "^3.0.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-animated-cursor": "^2.8.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2",
    "react-resize-detector": "^9.1.0",
    "react-router-dom": "^6.17.0",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "sass": "^1.59.3",
    "svgo": "^3.0.2",
    "terser": "^5.22.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.0",
    "vite-plugin-image-optimizer": "^1.1.2"
  }
}
